node{
  stage('scm checkout'){
   git'https://github.com/syeedashiza/mavenpro'
   }
   stage('compile-package'){
   sh'mvn clean install'
   }
  stage('email-notification'){
  mail bcc: '', body: '''hi welcome to email notification in jenkins build is success
  thanks and regards
  syeeda''', cc: '', from: '', replyTo: '', subject: 'jenkins-job', to: 'syeeda'
  }
}  
